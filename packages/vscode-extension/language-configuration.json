{
  "comments": {
    "lineComment": null,
    "blockComment": ["(", ")"]
  },
  "brackets": [
    ["[", "]"],
    ["{", "}"],
    ["(", ")"],
    ["<", ">"]
  ],
  "autoClosingPairs": [
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" },
    { "open": "(", "close": ")" },
    { "open": "<", "close": ">" }
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["{", "}"],
    ["(", ")"],
    ["<", ">"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\[",
      "end": "^\\s*$"
    }
  },
  "wordPattern": "[A-Za-z_][A-Za-z0-9_]*"
}
